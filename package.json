{
    "name": "aqc-charts",
    "version": "0.1.0",
    "description": "Modern Apache ECharts React components with TypeScript",
    "keywords": [
        "react",
        "echarts",
        "charts",
        "typescript",
        "visualization"
    ],
    "author": "Emil Lindfors <emil@aquacloud.ai>",
    "license": "MIT",
    "homepage": "https://github.com/aquacloudai/aqc-charts#readme",
    "repository": {
        "type": "git",
        "url": "https://github.com/aquacloudai/aqc-charts.git"
    },
    "bugs": {
        "url": "https://github.com/aquacloudai/aqc-charts/issues"
    },
    "type": "module",
    "main": "./dist/index.cjs",
    "module": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "exports": {
        ".": {
            "import": "./dist/index.js",
            "require": "./dist/index.cjs",
            "types": "./dist/index.d.ts"
        }
    },
    "files": [
        "dist",
        "README.md",
        "LICENSE"
    ],
    "sideEffects": false,
    "scripts": {
        "dev": "rolldown --config rolldown.config.js --watch",
        "build": "rolldown --config rolldown.config.js",
        "build:types": "tsc --emitDeclarationOnly",
        "test": "vitest run",
        "test:watch": "vitest",
        "test:ui": "vitest --ui",
        "test:coverage": "vitest run --coverage",
        "lint": "oxlint src",
        "lint:fix": "oxlint src --fix",
        "format": "dprint fmt",
        "format:check": "dprint check",
        "typecheck": "tsc --noEmit",
        "example": "cd example && bun run dev",
        "docs": "typedoc src/index.ts",
        "clean": "rm -rf dist",
        "prepublishOnly": "bun run clean && bun run build",
        "release": "np --no-yarn"
    },
    "peerDependencies": {
        "react": ">=18.0.0",
        "react-dom": ">=18.0.0"
    },
    "devDependencies": {
        "@rolldown/rolldown": "^0.6.2",
        "dprint": "^0.45.0",
        "oxlint": "^0.3.0",
        "@swc/core": "^1.3.102",
        "@testing-library/jest-dom": "^6.2.0",
        "@testing-library/react": "^14.1.2",
        "@testing-library/user-event": "^14.5.2",
        "@types/react": "^18.2.47",
        "@types/react-dom": "^18.2.18",
        "@vitest/coverage-v8": "^1.2.0",
        "@vitest/ui": "^1.2.0",
        "happy-dom": "^12.10.3",
        "np": "^9.2.0",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "typedoc": "^0.25.7",
        "typescript": "^5.3.3",
        "vite": "^5.0.11",
        "vitest": "^1.2.0"
    },
    "dependencies": {
        "echarts": "^5.6.0"
    },
    "engines": {
        "node": ">=18.0.0",
        "bun": ">=1.0.0"
    }
}